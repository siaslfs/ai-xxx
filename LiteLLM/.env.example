# --- LiteLLM Proxy Settings ---
LITELLM_MASTER_KEY=sk-change-me-to-a-strong-random-string    # Must start with sk-, use a strong random string
LITELLM_SALT_KEY=change-me-to-a-strong-random-string          # Used to encrypt keys in database
UI_USERNAME=admin
UI_PASSWORD=change-me-to-a-strong-password                    # Set a strong password

# --- Database Settings ---
POSTGRES_DB=LiteLLM
POSTGRES_USER=llmproxy
POSTGRES_PASSWORD=change-me-to-a-strong-password              # Set a strong password
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@your-db-host:5432/${POSTGRES_DB}

# --- LLM Provider API Keys ---
STORE_MODEL_IN_DB=true
# OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
ANTHROPIC_API_KEY=sk-ant-api03-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# COHERE_API_KEY=...

# --- (Optional) Redis Settings ---
# REDIS_HOST=your-redis-host
# REDIS_PORT=6379
# REDIS_PASSWORD=your-redis-password
